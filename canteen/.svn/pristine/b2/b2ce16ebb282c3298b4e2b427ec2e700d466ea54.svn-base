package com.winhands.modules.restaurant.entity;


import java.io.Serializable;
import java.math.BigDecimal;
/**
 * 菜品管理
 */
public class AdvanceDishesEntity implements Serializable {
	private static final long serialVersionUID = 1L;
	private Long id; // ID
	private String advanceDishes;//菜名
	private BigDecimal advDishesPrice;
	private String typeName;//种类
	private String advanceDishesPicPath;//菜图片路径
	private String advanceDishesPicName;//菜图片原名
	private String batching;//配料
	private String instruction;//说明
	private String address;//属地
	private Long createUserId;
	private String advanceDetailHtml;
	private String advanceDishesDetail;
	private Integer counts;
	public Integer getCounts() {
		return counts;
	}

	public void setCounts(Integer counts) {
		this.counts = counts;
	}


	public BigDecimal getAdvDishesPrice() {
		return advDishesPrice;
	}

	public void setAdvDishesPrice(BigDecimal advDishesPrice) {
		this.advDishesPrice = advDishesPrice;
	}

	public String getAdvanceDishesDetail() {
		return advanceDishesDetail;
	}

	public void setAdvanceDishesDetail(String advanceDishesDetail) {
		this.advanceDishesDetail = advanceDishesDetail;
	}

	public String getAdvanceDishesPicPath() {
		return advanceDishesPicPath;
	}

	public void setAdvanceDishesPicPath(String advanceDishesPicPath) {
		this.advanceDishesPicPath = advanceDishesPicPath;
	}

	public String getAdvanceDishesPicName() {
		return advanceDishesPicName;
	}

	public void setAdvanceDishesPicName(String advanceDishesPicName) {
		this.advanceDishesPicName = advanceDishesPicName;
	}

	public String getAdvanceDetailHtml() {
		return advanceDetailHtml;
	}

	public void setAdvanceDetailHtml(String advanceDetailHtml) {
		this.advanceDetailHtml = advanceDetailHtml;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getAdvanceDishes() {
		return advanceDishes;
	}

	public void setAdvanceDishes(String advanceDishes) {
		this.advanceDishes = advanceDishes;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}



	public String getBatching() {
		return batching;
	}

	public void setBatching(String batching) {
		this.batching = batching;
	}

	public String getInstruction() {
		return instruction;
	}

	public void setInstruction(String instruction) {
		this.instruction = instruction;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public Long getCreateUserId() {
		return createUserId;
	}

	public void setCreateUserId(Long createUserId) {
		this.createUserId = createUserId;
	}

	
	
}
