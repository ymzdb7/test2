package com.winhands.modules.restaurant.entity;
import java.io.Serializable;
import java.sql.Timestamp;
/**
 * 环境评论管理
 */
public class EnviroTalkEntity implements Serializable {
	private static final long serialVersionUID = 1L;
	
	/**
	 * ID
	 */
	private Long id;

	/**
	 * 评论人
	 */
	private String userName;

	/**
	 * 属地
	 */

	private String address;

	/**
	 * 几颗星
	 */

	private Integer startNum;

	/**
	 * 随性数变化的评论内容
	 */

	private String startEvaluate;

	/**
	 * 评论人表达的内容
	 */

	private String content;
	
	/**
	 * 评论人表达的内容
	 */
	
	private Timestamp createTime;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public Integer getStartNum() {
		return startNum;
	}

	public void setStartNum(Integer startNum) {
		this.startNum = startNum;
	}

	public String getStartEvaluate() {
		return startEvaluate;
	}

	public void setStartEvaluate(String startEvaluate) {
		this.startEvaluate = startEvaluate;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Timestamp getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Timestamp createTime) {
		this.createTime = createTime;
	}
	

}
